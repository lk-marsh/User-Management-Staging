		{
			"name": "Get User Group",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const tokenUrl = 'https://staging2.api.m2digitalbroker.com/proxy/amps/v2/oauth/accesstoken';\r",
							"\r",
							"const clientId = 'Y1rJeO1VJyh9VpOTtyiDYgV2AAvQ6TWY';\r",
							"const clientSecret = 'nC3JsHlQkp2vAS53';\r",
							"\r",
							"const getTokenRequest = {\r",
							"  method: 'POST',\r",
							"  url: tokenUrl,\r",
							"  body: {\r",
							"      mode: 'formdata',\r",
							"      formdata: [\r",
							"          { key: 'grant_type', value: 'client_credentials' },\r",
							"          { key: 'appId', value: 'VMP-DI' },\r",
							"          { key: 'client_id', value: clientId },\r",
							"          { key: 'client_secret', value: clientSecret }\r",
							"      ]\r",
							"  }\r",
							"};\r",
							"\r",
							"pm.sendRequest(getTokenRequest, (err, response) => {\r",
							"  const jsonResponse = response.json();\r",
							"  const newAccessToken = jsonResponse.access_token;\r",
							"  \r",
							"  pm.variables.set('access_token', newAccessToken);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
